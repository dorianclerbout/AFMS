<!DOCTYPE html>

<title>Formation</title>

{% block body %}
    <h1>Formation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ formation.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ formation.name }}</td>
            </tr>
            <tr>
                <th>Time</th>
                <td>{{ formation.time }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ formation.description }}</td>
            </tr>
            <tr>
                <th>People</th>
                <td>{{ formation.people }}</td>
            </tr>
            <tr>
                <th>Categorie</th>
                <td>{{ formation.categorie }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_formation_index') }}">back to list</a>

    <a href="{{ path('app_formation_edit', {'id': formation.id}) }}">edit</a>

    {{ include('formation/_delete_form.html.twig') }}
{% endblock %}
